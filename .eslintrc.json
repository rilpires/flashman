{
    "extends": ["eslint:recommended", "google"],
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jquery": true,
        "jest": true
    },
    "plugins": [
        "jest"
    ],
    "rules": {
       "no-undef": 2,
       "no-invalid-this": "off",
       "dot-location": ["error", "property"]
    }
}
